<?xml version="1.0" encoding="UTF-8"?>
<rootconfig>
  <tests>
    <test name="StubMarker">
      <option name="severity"><value>brutal</value></option>
      <option name="pityMarks"><value>0</value></option>
      <option name="MarksAvailable"><value>1</value></option>
    </test>
    <test name="SimpleMarker">
      <option name="MarksAvailable"><value>5</value></option>
      <option name="FeedbackMessage"><value>Good Work!</value></option>
      <option name="BonusMarks"><value>2</value></option>
    </test>
    <test name="VowelCounter">
      <option name="MarksAvailable"><value>1</value></option>
      <option name="caseInsensitive"><value>true</value></option>
      <option name="badOption"></option>
    </test>
  </tests>
  <output name="DebugLogger">
  </output>
</rootconfig> 